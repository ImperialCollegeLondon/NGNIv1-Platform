<Window x:Class="USBridge_WPF.TabHostWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:dragablz="clr-namespace:Dragablz;assembly=Dragablz"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" mc:Ignorable="d"
        xmlns:local="clr-namespace:USBridge_WPF"
        Title="TabHostWindow" Height="300" Width="300"
        d:DataContext="{d:DesignInstance  local:MainWindowViewModel, IsDesignTimeCreatable=False}">
    <Grid>
        <dragablz:TabablzControl Name="TabsContainer">
            <dragablz:TabablzControl.InterTabController>
                <dragablz:InterTabController InterTabClient="{Binding MainInterTabClient}">
            </dragablz:TabablzControl.InterTabController>
        </dragablz:TabablzControl>
    </Grid>
</Window>
